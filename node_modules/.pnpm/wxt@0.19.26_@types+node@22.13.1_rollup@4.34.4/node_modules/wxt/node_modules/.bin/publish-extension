#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/publish-browser-extension@2.3.0/node_modules/publish-browser-extension/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/publish-browser-extension@2.3.0/node_modules/publish-browser-extension/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/publish-browser-extension@2.3.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/publish-browser-extension@2.3.0/node_modules/publish-browser-extension/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/publish-browser-extension@2.3.0/node_modules/publish-browser-extension/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/publish-browser-extension@2.3.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../publish-browser-extension@2.3.0/node_modules/publish-browser-extension/bin/publish-extension.cjs" "$@"
else
  exec node  "$basedir/../../../../../publish-browser-extension@2.3.0/node_modules/publish-browser-extension/bin/publish-extension.cjs" "$@"
fi
