#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/firefox-profile@4.6.0/node_modules/firefox-profile/lib/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/firefox-profile@4.6.0/node_modules/firefox-profile/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/firefox-profile@4.6.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/firefox-profile@4.6.0/node_modules/firefox-profile/lib/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/firefox-profile@4.6.0/node_modules/firefox-profile/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/firefox-profile@4.6.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../firefox-profile@4.6.0/node_modules/firefox-profile/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../firefox-profile@4.6.0/node_modules/firefox-profile/lib/cli.js" "$@"
fi
