#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/fx-runner@1.4.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/fx-runner@1.4.0/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../fx-runner@1.4.0/node_modules/fx-runner/bin/fx-runner" "$@"
else
  exec node  "$basedir/../../../../../fx-runner@1.4.0/node_modules/fx-runner/bin/fx-runner" "$@"
fi
