#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/rollup-plugin-visualizer/dist/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/rollup-plugin-visualizer/dist/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/rollup-plugin-visualizer/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/rollup-plugin-visualizer/dist/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/rollup-plugin-visualizer/dist/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/rollup-plugin-visualizer/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules/@aklinker1/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@aklinker1+rollup-plugin-visualizer@5.12.0_rollup@4.34.4/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../../dist/bin/cli.js" "$@"
fi
